{
	"name": "line-logger",
	"displayName": "line-logger",
	"description": "This extension for VS Code writes the line numbers next to specific comment tokens, so you can e.g. log them to the console",
	"version": "0.3.2",
	"engines": {
		"vscode": "^1.44.0"
	},
	"keywords": [
		"line number",
		"line numbers"
	],
	"categories": [],
	"author": {
		"name": "Sebastian Aatz",
		"email": "s.aatz@gmx.de"
	},
	"publisher": "TODO:",
	"repository": {
		"type": "git",
		"url": "https://github.com/Huegelkoenig/line-logger-VSCE.git",
		"homepage": "https://github.com/Huegelkoenig/line-logger-VSCE"
	},
	"bugs": {
		"url": "https://github.com/Huegelkoenig/line-logger-VSCE/issues"
	},
	"icon": "https://github.com/Huegelkoenig/line-logger-VSCE/graphics/line-logger-icon.png",
	"license": "MIT",
	"activationEvents": [
		"onCommand:line-logger.log-left",
		"onCommand:line-logger.log-right",
		"onCommand:line-logger.delete-left",
		"onCommand:line-logger.delete-right",
		"onCommand:line-logger.erase-tokens"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "line-logger.log-left",
				"title": "log line numbers to the left of the line-logger tokens"
			},
			{
				"command": "line-logger.log-right",
				"title": "log line numbers to the right of the line-logger tokens"
			},
			{
				"command": "line-logger.delete-left",
				"title": "delete line numbers adjacent on the left of line-logger tokens"
			},
			{
				"command": "line-logger.delete-right",
				"title": "delete line numbers adjacent on the right of line-logger tokens"
			},
			{
				"command": "line-logger.erase-tokens",
				"title": "erase all line-logger tokens"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.4",
		"@types/vscode": "^1.44.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
